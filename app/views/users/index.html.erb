<h2> Users </h2>
<table class="table table-hover">
  <thead>
  <tr>
    <th scope="col">#</th>
    <th scope="col">Email</th>
    <th scope="col">Name</th>
    <th scope="col">Admin?</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <th scope="row"><%= user.id %></th>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
      <td><%= user.admin? %></td>
    </tr>
  <% end %>
  </tbody>
</table>

